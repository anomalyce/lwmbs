            "match": "libffi.+\\.tar\\.gz"
        },
        "libzip": {
            "type": "ghrel",
            "repo": "nih-at/libzip",
            "match": "libzip.+\\.tar\\.xz"
        },
        "curl": {
            "type": "ghrel",
            "repo": "curl/curl",
            "match": "curl.+\\.tar\\.xz"
        },
        "brotli": {
